<?xml version="1.0" encoding="windows-1251" ?>
<fileroot>
  <Resources fileName="GUI_textures_test.xml"/>
  <BykerGame/>
  <IButton name="load" x="0" y="0" width="40" height="40" caption="Load">
    SendMessage("tile_editor", "open");
  </IButton>
  <IButton name="play" x="60" y="0" width="40" height="40" caption="play">
    SendMessage("tile_editor", "play");
  </IButton>
  <IButton name="random" x="110" y="0" width="40" height="40" caption="random">
    SendMessage("tile_editor", "random");
  </IButton>
  <ButtonList name="SmallList" receiver="tile_editor" x="140" y="40" width="70" height="40" stepDown="43" stepRight="43" itemsInRow="1"/>  
  <ButtonList name="BigList" receiver="tile_editor" x="20" y="45" width="40" height="40" stepDown="50" stepRight="50" itemsInRow="8"/>
  <MyMessageBox x="360" y="100" width="240" height="100" textY="10">
    <Ok name="Ok" x="450" y="150" width="60" height="40" caption="ok">
      SendMessage("MessageBox", "ok");
    </Ok>
    <Yes name="Yes" x="400" y="150" width="60" height="40" caption="yes">
      SendMessage("MessageBox", "yes");
    </Yes>
    <No name="No" x="500" y="150" width="60" height="40" caption="no">
      SendMessage("MessageBox", "no");
    </No>
  </MyMessageBox>
  <FpsCounter/>
</fileroot>